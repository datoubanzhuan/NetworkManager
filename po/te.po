"  status\n"
"\n"
"  show [<ifname>]\n"
"\n"
"  disconnect <ifname> [--nowait] [--timeout <timeout>]\n"
"\n"
"  wifi [list [ifname <ifname>] [bssid <BSSID>]]\n"
"\n"
